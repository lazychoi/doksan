   "source": [
    "# 두 정수 사이의 합\n",
    "\n",
    "def solution(a, b):\n",
    "    answer = 0\n",
    "    if a > b:\n",
    "        a, b = b, a\n",
    "    nums = [ i for i in range(a, b + 1)]\n",
    "    answer = sum(nums)    \n",
    "    \n",
    "    return answer"
   ]
